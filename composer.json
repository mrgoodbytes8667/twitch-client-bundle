{
    "name": "mrgoodbytes8667/twitch-client-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that adds HttpClient (and other) mechanisms for Twitch",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.12",
        "doctrine/collections": "^1.6",
        "mrgoodbytes8667/twitch-response-bundle": "^0.7.2",
        "symfony/polyfill-uuid": "^1.20"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "fakerphp/faker": "^1.13",
        "mrgoodbytes8667/test-common": ">=0.3",
        "mrgoodbytes8667/test-twitch-faker": ">=0.0.5",
        "rector/rector": "^0.15",
        "symfony/security-bundle": "^6.2",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^6.2"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "suggest": {
        "symfony/security-bundle": ""
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "mrgoodbytes8667/enum-serializer-bundle": "<3.1.0",
        "mrgoodbytes8667/test-common-faker": "<0.4.4",
        "phpunit/phpunit": "<9.5",
        "symfony/framework-bundle": "<5.3.0",
        "symfony/http-client": "<5.3.0",
        "symfony/serializer": "<5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\TwitchClientBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bytes\\TwitchClientBundle\\Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev"
}
